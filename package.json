{
  "name": "composition-logger",
  "version": "2.0.1",
  "description": "A browser first utility tool for logging each step within a functional composition",
  "main": "distribution/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/grady-lad/composition-logger.git"
  },
  "scripts": {
    "build": "BABEL_ENV=build rollup -c",
    "start": "rollup -c -w",
    "test": " BABEL_ENV=test jest",
    "test:watch": "BABEL_ENV=test jest --watch",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "author": "Martin O'Grady (https://github.com/grady-lad)",
  "license": "ISC",
  "keywords": [
    "functional",
    "composition",
    "debugging",
    "logging",
    "fp",
    "console",
    "log",
    "group"
  ],
  "jest": {
    "testRegex": "__tests__/.*\\.test\\.js$"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.12.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.3"
  }
}
